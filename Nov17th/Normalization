-- 2NF: Question 1

create table Student1_2nf(studentid numeric primary key,studentname varchar(20));
create table course1_2nf(courseid numeric primary key,coursename varchar(20));
create table student1_course1_2nf(studentid numeric,courseid numeric,faculty varchar(20),marks numeric,primary key(studentid,courseid));

